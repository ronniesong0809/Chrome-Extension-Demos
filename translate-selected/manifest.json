{
    "manifest_version": 2,
    "name": "Google Translate",
    "version": "1.0",
    "description": "Translate selected",
    "background": {
        "scripts": [
            "js/script.js"
        ]
    },
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["js/content_script.js"]
    }],
    "permissions": [
        "contextMenus"
    ]
}
